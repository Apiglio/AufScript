//非等长计算测试
define a, $8[00]
define b, $3[08]

fill @a, 0
fill @b, 255
print "A:   "
binln @a
print "B:   "
binln @b
or @a, @b
print "A|B: "
binln @a

cwln

fill @a, 255
fill @b, 255
print "A:   "
binln @a
print "B:   "
binln @b
and @a, @b
print "A&B: "
binln @a

cwln

fill @a, 255
fill @b, 255
print "A:   "
binln @a
print "B:   "
binln @b
xor @a, @b
print "A^B: "
binln @a

