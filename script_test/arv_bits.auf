//位移测试
define a, $8[00]
define b, $8[08]

fill @a, 255
print "A:     "
binln @a
shl @a 12
print "A<<12: "
binln @a

fill @b, 255
print "B:     "
binln @b
shr @b 24
print "B>>24: "
binln @b

cwln
